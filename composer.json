    "homepage": "http://github.com/GoldenBirdLabs/ReplaceAssetsBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Júlio César Lacerda",
            "email": "julio.zero@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2"
    },
    "require-dev": {
        "symfony/framework-bundle": ">=2.1,<2.2-dev"
    },
    "autoload": {
        "psr-0": { "GoldenBirdLabs\\ReplaceAssetsBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "GoldenBirdLabs/ReplaceAssetsBundle"
